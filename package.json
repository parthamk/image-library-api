{
  "name": "image-library-api",
  "version": "1.0.0",
  "description": "Image Library API - Full Stack Project",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=server & npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "build": "npm run build --workspace=client && npm run build --workspace=server",
    "build:server": "npm run build --workspace=server",
    "build:client": "npm run build --workspace=client",
    "start": "npm start --workspace=server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parthamk/image-library-api.git"
  },
  "keywords": [
    "api",
    "images",
    "github",
    "documentation"
  ],
